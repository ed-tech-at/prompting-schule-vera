{
	"name": "sveltekit",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"start": "vite preview  --host --port 3000",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^5.2.11",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.4",
		"jsdom": "^26.0.0",
		"prisma": "^6.5.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^6.0.0",
		"vitest": "^3.0.0"
	},
	"dependencies": {
		"@azure/openai": "^2.0.0",
		"@prisma/client": "^6.5.0",
		"bcrypt": "^6.0.0",
		"dotenv": "^16.4.7",
		"jsonwebtoken": "^9.0.2",
		"marked": "^15.0.7",
		"nodemailer": "^7.0.13",
		"openai": "^4.91.1",
		"openbadges-bakery-v3": "^1.0.6",
		"pdf-lib": "^1.17.1",
		"qrcode": "^1.5.4",
		"sharp": "^0.34.0",
		"uuid": "^11.1.0"
	},
	"overrides": {
		"openbadges-bakery-v3": {
			"jws": "^3.2.3"
		}
	}
}
